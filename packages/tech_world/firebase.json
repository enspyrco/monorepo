{
  "firestore": {
    "rules": ".firebase/firestore.rules"
  },
  "hosting": {
    "predeploy": "flutter build web && cp web/index.auto build/web/index.html",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
