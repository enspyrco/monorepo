rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }

    match /users/{userId} {
      allow read, write: if request.auth.uid == userId;
    }

    match /users/{userId}/credentials/{providerId} {
      allow read, write: if request.auth.uid == userId;
    }
  }
}